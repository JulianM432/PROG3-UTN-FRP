package tp0000001;


public class Persona implements Comparable<Persona>{
	int id;
	int edad;
	int dni;
	String nombre;
	String apellido;

	Persona(int id, int edad, int dni, String nombre, String apellido) {
		this.id = id;
		this.edad = edad;
		this.dni = dni;
		this.nombre = nombre;
		this.apellido = apellido;
	}
	
	public int compareTo() {
		int result = 0;
		if (nombre.compareTo(nombre)) {
			return 1;
		} else if (this.getEdad() < o.getEdad()) {
			return -1;
		} else {
			return 0;
		}
		return result;
	}
}
